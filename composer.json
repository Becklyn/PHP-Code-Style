{
    "name": "becklyn/php-cs",
    "type": "lib",
    "description": "Config files for PHP code style",
    "homepage": "https://github.com/Becklyn/PHP-Code-Style",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.1",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.11.8",
        "phpstan/phpstan-doctrine": "^0.11.5",
        "phpstan/phpstan-symfony": "^0.11.6",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/orm": "^2.6",
        "php-translation/extractor": "^1.7",
        "symfony/form": "^4.3",
        "symfony/translation": "^4.3",
        "symfony/validator": "^4.3",
        "twig/twig": "^2.2"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "To use the translation extractor.",
        "doctrine/orm": "To use the SchemaValidationTest.",
        "php-translation/extractor": "To use the translation extractor.",
        "symfony/form": "To use the translation extractor.",
        "symfony/translation": "To use the translation extractor.",
        "symfony/validator": "To use the translation extractor.",
        "twig/twig": "To use the translation extractor."
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\PhpCs\\": "src/"
        }
    }
}
