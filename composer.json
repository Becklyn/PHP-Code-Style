{
    "name": "becklyn/php-cs",
    "type": "lib",
    "description": "Config files for PHP code style",
    "homepage": "https://github.com/Becklyn/PHP-Code-Style",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.1",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.11.8",
        "phpstan/phpstan-doctrine": "^0.11.5",
        "phpstan/phpstan-symfony": "^0.11.6",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.6"
    },
    "suggest": {
        "doctrine/orm": "To use the SchemaValidationTest."
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\PhpCs\\": "src/"
        }
    },
    "bin": [
        "bin/fix-ci"
    ]
}
