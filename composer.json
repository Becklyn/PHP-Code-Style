{
    "name": "becklyn/php-cs",
    "type": "lib",
    "description": "Config files for PHP code style",
    "homepage": "https://github.com/Becklyn/PHP-Code-Style",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\PhpCs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\PhpCs\\": "tests/"
        }
    }
}
